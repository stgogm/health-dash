{
  "compilerOptions": {
    "moduleResolution": "Node",
    "outDir": "./dist",
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": ".",
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "paths": {
      "@health/shared": ["packages/shared"]
    }
  }
}
