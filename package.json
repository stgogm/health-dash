{
  "name": "nestjs-practice",
  "version": "1.0.0",
  "description": "NestJs Practice Repo",
  "main": "dist/src/main",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stgogm/nestjs-practice.git"
  },
  "engines": {
    "npm": ">=10.0.0",
    "node": ">=20.0.0"
  },
  "keywords": [],
  "author": "@stgogm",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stgogm/nestjs-practice/issues"
  },
  "homepage": "https://github.com/stgogm/nestjs-practice#readme",
  "dependencies": {
    "@nestjs/common": "^11.0.20",
    "@nestjs/core": "^11.0.20",
    "@nestjs/platform-express": "^11.0.20",
    "@nestjs/typeorm": "^11.0.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.22"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.20",
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.21",
    "@swc/jest": "^0.2.37",
    "@types/node": "^22.14.1",
    "jest": "^29.7.0",
    "typescript": "^5.8.3"
  }
}
